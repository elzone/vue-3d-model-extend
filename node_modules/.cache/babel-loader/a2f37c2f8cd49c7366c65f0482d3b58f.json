   <model-obj\n        :backgroundAlpha=\"0\"\n        ref=\"model\"\n        @on-load=\"onLoad\"\n        @on-mousemove=\"onMouseMove\"\n        src=\"static/models/obj/tree.obj\"\n      ></model-obj>\n      <div class=\"example-loading\" v-show=\"loading\"></div>\n    </template>\n  </demo-block>\n</template>\n\n<script>\nimport DemoBlock from '../components/demo-block.vue';\nimport ModelObj from '../../src/model-obj.vue';\n\nconst code = `\n\n<template>\n    <model-obj \n        src=\"static/models/obj/tree.obj\"\n        @on-mousemove=\"onMouseMove\">\n    </model-obj>\n</template>\n\n<script>\n    import { ModelObj } from 'vue-3d-model'\n\n    export default {\n        data () {\n            return {\n                intersected: null\n            }\n        },\n        methods: {\n            onMouseMove ( event ) {\n\n                console.log( event );   // event: { distance, face, faceIndex, point, index, uv, object }\n\n                const { camera, intersected } = { ...event };\n\n                if (!intersected) {\n                  if (this.intersected) {\n                    this.intersected.material.color.setStyle('#fff');\n                  }\n\n                  this.intersected = null;\n                  return;\n                }\n\n                if (intersected) {\n                  this.intersected = intersected.object;\n                  console.log(this.intersected);\n                  this.intersected.material.color.setStyle('#13ce66');\n                }\n                if (camera) {\n                  console.log(event.camera);\n                }\n            }\n        },\n        components: {\n            ModelObj\n        }\n    }\n<\\/script>\n\n`;\n\nconst htmlCode = `\n<body>\n    <div id=\"app\">\n        <model-obj \n            src=\"static/models/obj/tree.obj\"\n            @on-mousemove=\"onMouseMove\">\n        </model-obj>\n    </div>\n    #scripts#\n    <script>\n        new Vue({\n            el: '#app',\n            data: {\n                intersected: null\n            },\n            methods: {\n                onMouseMove ( event ) {\n\n                    console.log(event);\n                    const { camera, intersected } = { ...event };\n\n                    if (!intersected) {\n                      if (this.intersected) {\n                        this.intersected.material.color.setStyle('#fff');\n                      }\n\n                      this.intersected = null;\n                      return;\n                    }\n\n                    if (intersected) {\n                      this.intersected = intersected.object;\n                      console.log(this.intersected);\n                      this.intersected.material.color.setStyle('#13ce66');\n                    }\n                    if (camera) {\n                      console.log(event.camera);\n                    }\n                }\n            }\n        })\n    <\\/script>\n</body>\n`;\n\nexport default {\n  name: 'demo-event',\n  data() {\n    return {\n      code,\n      htmlCode,\n      loading: true,\n      intersected: null,\n    };\n  },\n  methods: {\n    onLoad() {\n      this.loading = false;\n    },\n    onMouseMove(event) {\n      console.log(event);\n      const { camera, intersected } = { ...event };\n\n      if (!intersected) {\n        if (this.intersected) {\n          this.intersected.material.color.setStyle('#fff');\n        }\n\n        this.intersected = null;\n        return;\n      }\n\n      if (intersected) {\n        this.intersected = intersected.object;\n        console.log(this.intersected);\n        this.intersected.material.color.setStyle('#13ce66');\n      }\n      if (camera) {\n        console.log(event.camera);\n      }\n    },\n  },\n  components: {\n    ModelObj,\n    DemoBlock,\n  },\n};\n</script>\n"]}]}